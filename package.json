{
  "name": "@knovator/novu-connector",
  "version": "1.0.0",
  "license": "MIT",
  "author": "knovator (https://knovator.com/)",
  "main": "dist/index.js",
  "module": "dist/novu-connector.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:knovator/masters-node.git"
  },
  "keywords": [
    "nodejs"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/novu-connector.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/novu-connector.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@tsconfig/recommended": "^1.0.1",
    "dts-cli": "^1.5.2",
    "husky": "^8.0.1",
    "size-limit": "^7.0.8",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^0.27.2"
  }
}
